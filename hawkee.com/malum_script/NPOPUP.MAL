.Op:/raw -q mode # +o $1
.Op all selected users: {
  %i = 0
  %nicks = ""
  :nextnick
  inc %i
  if ($snick(%i,#) == $null) { if ($len(%nicks) > 0) raw -q mode # +oooo %nicks | halt }
  %nicks = %nicks $snick(%i,#)
  if (4 // %i) { raw -q mode # +oooo %nicks | %nicks = "" }
  goto nextnick
}
