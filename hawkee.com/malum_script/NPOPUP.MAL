MASS
.Mass Kill: {
  %mkillmessage = $$?="Enter Kill Mesage:"
  %mkillnum = 1
  :start
  if ($snick(%mkillnum,#) == $me) goto next
  kill $snick(%mkillnum,#) %mkillmessage
  goto next
  :next
  inc %mkillnum
  if ($snick(%mkillnum,#) == $null) unset %mkill* | halt
  elseif $snick(%mkillnum,#) != $null) goto start
}
.Mass Deop: {
  %i = 0
  %nicks = ""
  :nextnick
  inc %i
  if ($snick(%i,#) == $me) inc %i
  if ($snick(%i,#) == $null) { if ($len(%nicks) > 0) raw -q mode # -oooooo %nicks | halt }
  %nicks = %nicks $snick(%i,#)
  if (6 // %i) { raw -q mode # -oooooo %nicks | %nicks = "" }
  goto nextnick
}
